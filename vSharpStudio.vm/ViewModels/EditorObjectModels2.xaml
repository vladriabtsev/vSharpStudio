<UserControl xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"  x:Class="vSharpStudio.vm.ViewModels.EditorObjectModels2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:vSharpStudio.vm.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <xctk:CheckListBox 
                         ItemsSource="{Binding ListModels}"
                         Delimiter=","
                         SelectedMemberPath="{Binding IsSelected}"
                         DisplayMemberPath="{Binding Name}"
                         IsSelectAllActive="False"
            />
        <!--ValueMemberPath="{Binding ElementName=_valueMemberPath, Path=SelectedItem}"
        SelectAllContent="{Binding ElementName=_selectAllContent, Path=Text}"-->
    </Grid>
</UserControl>
