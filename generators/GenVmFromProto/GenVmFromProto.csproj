  -->
  <Target Name="ProtoToVmGen" AfterTargets="Build" Condition=" '@(Protobuf_Compile)' != '' " Inputs="@(Protobuf_Compile->'$(SolutionDir)doc\%(Filename).json')" Outputs="%(Protobuf_Compile-&gt;'$(ProjectDir)Generated\%(Filename)ProtoViewModels.cs')">
    <!--https://github.com/grpc/grpc/blob/master/src/csharp/Grpc.Tools/build/_protobuf/Google.Protobuf.Tools.targets-->
    <PropertyGroup>
      <SharpStudioRelativePath></SharpStudioRelativePath>
      <ProtoName>@(Protobuf_Compile->'%(Filename)')</ProtoName>
      <ProtoFileDir>@(Protobuf_Compile->'%(RootDir)')@(Protobuf_Compile->'%(Directory)')</ProtoFileDir>
      <ProtoFile>$(ProtoName).proto</ProtoFile>
      <ProtoDocGenPath>$(SolutionDir)$(SharpStudioRelativePath)protoc\protoc-gen-doc.exe</ProtoDocGenPath>
      <DocGenOutPath>$(SolutionDir)doc</DocGenOutPath>
      <OutDirGenVm>bin\Debug\net8.0\</OutDirGenVm>
      <IsGen></IsGen>
    </PropertyGroup>
    <PropertyGroup Condition="$(ProtoName)=='vsharpstudio'">
      <IsGen>true</IsGen>
      <VmNamespace>vSharpStudio.vm.ViewModels</VmNamespace>
      <VmInterfacesNamespace>vSharpStudio.common</VmInterfacesNamespace>
      <ProtoDocGenCommandVm>$(SolutionDir)$(SharpStudioRelativePath)generators\GenVmFromProto\$(OutDirGenVm)GenVmFromProto.exe -r -m -p $(ProtoName) -o $(ProjectDir)..\..\vSharpStudio.vm\Generated\$(ProtoName)ProtoViewModels.cs -n $(VmNamespace) -d $(DocGenOutPath)\</ProtoDocGenCommandVm>
      <ProtoDocGenCommandVmInterface>$(SolutionDir)$(SharpStudioRelativePath)generators\GenVmFromProto\$(OutDirGenVm)GenVmFromProto.exe -r -i -p $(ProtoName) -o $(ProjectDir)..\..\vSharpStudio.common\Generated\$(ProtoName)ProtoViewModelInterfaces.cs -n $(VmInterfacesNamespace) -d $(DocGenOutPath)\</ProtoDocGenCommandVmInterface>
    </PropertyGroup>
    <PropertyGroup Condition="$(ProtoName)=='plugin_sample'">
      <IsGen>true</IsGen>
      <VmNamespace>vPlugin.Sample</VmNamespace>
      <VmInterfacesNamespace>vPlugin.Sample</VmInterfacesNamespace>
      <ProtoDocGenCommandVm>$(SolutionDir)$(SharpStudioRelativePath)generators\GenVmFromProto\$(OutDirGenVm)GenVmFromProto.exe -r -m -p $(ProtoName) -o $(ProjectDir)..\..\vPlugin.Sample\Generated\$(ProtoName)ProtoViewModels.cs -n $(VmNamespace) -d $(DocGenOutPath)\ -b ConfigObjectVmGenSettings</ProtoDocGenCommandVm>
      <ProtoDocGenCommandVmInterface>$(SolutionDir)$(SharpStudioRelativePath)generators\GenVmFromProto\$(OutDirGenVm)GenVmFromProto.exe -r -i -p $(ProtoName) -o $(ProjectDir)..\..\vPlugin.Sample\Generated\$(ProtoName)ProtoViewModelInterfaces.cs -n $(VmInterfacesNamespace) -d $(DocGenOutPath)\ -b ConfigObjectVmGenSettings</ProtoDocGenCommandVmInterface>
    </PropertyGroup>
    <PropertyGroup Condition="$(ProtoName)=='plugin_sample2'">
      <IsGen>true</IsGen>
      <VmNamespace>vPlugin.Sample2</VmNamespace>
      <VmInterfacesNamespace>vPlugin.Sample2</VmInterfacesNamespace>
      <ProtoDocGenCommandVm>$(SolutionDir)$(SharpStudioRelativePath)generators\GenVmFromProto\$(OutDirGenVm)GenVmFromProto.exe -r -m -p $(ProtoName) -o $(ProjectDir)..\..\vPlugin.Sample2\Generated\$(ProtoName)ProtoViewModels.cs -n $(VmNamespace) -d $(DocGenOutPath)\ -b ConfigObjectVmGenSettings</ProtoDocGenCommandVm>
      <ProtoDocGenCommandVmInterface>$(SolutionDir)$(SharpStudioRelativePath)generators\GenVmFromProto\$(OutDirGenVm)GenVmFromProto.exe -r -i -p $(ProtoName) -o $(ProjectDir)..\..\vPlugin.Sample2\Generated\$(ProtoName)ProtoViewModelInterfaces.cs -n $(VmInterfacesNamespace) -d $(DocGenOutPath)\ -b ConfigObjectVmGenSettings</ProtoDocGenCommandVmInterface>
    </PropertyGroup>
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)****************** Start Proto To VM Generation ********************" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**              ProtoName: $(ProtoName)" />
    <!--<Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**               Protobuf: @(Protobuf)" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**       Protobuf_Compile: @(Protobuf_Compile)" />-->
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**                  Input: @(Protobuf_Compile->'$(SolutionDir)doc\%(Filename).json')" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**                 Output: @(Protobuf_Compile->'$(ProjectDir)Generated\%(Filename)ProtoViewModels.cs')" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**            OutDirGenVm: $(OutDirGenVm)" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**SharpStudioRelativePath: $(SharpStudioRelativePath)" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**          DocGenOutPath: $(DocGenOutPath)" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**              CommandVm: $(ProtoDocGenCommandVm)" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)**     CommandVmInterface: $(ProtoDocGenCommandVmInterface)" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)** Generating proto VM ..." />
    <Exec Command="$(ProtoDocGenCommandVm)" Condition="$(IsGen)=='true'" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)** Generating proto VM Interfaces ..." />
    <Exec Command="$(ProtoDocGenCommandVmInterface)" Condition="$(IsGen)=='true'" />
    <Message Importance="high" Condition="$(IsGen)=='true'" Text="$(ProjectName)****************** End Proto To VM Generation **********************" />
  </Target>
