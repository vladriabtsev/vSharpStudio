    <HierarchicalDataTemplate DataType="{x:Type dat:Config}">
        <StackPanel Orientation="Horizontal">
            <Grid>
                <ContentControl Template="{Binding IconFolder, Converter={StaticResource ConverterIconNameToResource}, Mode=OneWay}" />
                <Viewbox Height="8" Width="8" VerticalAlignment="Bottom" HorizontalAlignment="Right">
                    <ContentControl Template="{Binding IconStatus, Converter={StaticResource ConverterIconNameToResource}, Mode=OneWay}" />
                </Viewbox>
            </Grid>
            <TextBlock Text="{Binding Name}" Margin="2,0,0,2" />
            <StackPanel VerticalAlignment="Center">
                <Rectangle Fill="Red" Height="4" Width="{Binding CountErrors, Converter={StaticResource ConverterCountToWidth}, Mode=OneWay}" VerticalAlignment="Bottom" />
                <Rectangle Fill="#FFFBA104" Height="4" Width="{Binding CountWarnings, Converter={StaticResource ConverterCountToWidth}, Mode=OneWay}" VerticalAlignment="Bottom" Margin="0,1,0,1" />
                <Rectangle Fill="#FF04EEF9" Height="4" Width="{Binding CountInfos, Converter={StaticResource ConverterCountToWidth}, Mode=OneWay}" VerticalAlignment="Bottom" />
            </StackPanel>
        </StackPanel>
    </HierarchicalDataTemplate>
